package de.dis2012.core.log;

import java.util.Hashtable;

public class Puffer {
	Hashtable<int, Puffer> puffer = new Hashtable<int, Puffer>();

	public Hashtable<String, Puffer> getPuffer() {
		return puffer;
	}

	public void setPuffer(Hashtable<String, Puffer> puffer) {
		this.puffer = puffer;
	}
	
	
}
