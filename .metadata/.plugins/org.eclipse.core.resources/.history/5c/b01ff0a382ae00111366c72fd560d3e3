package de.dis2012.core.log;

import java.util.Hashtable;

public class Puffer {
	//private Hashtable<String, Puffer> puffer = new Hashtable<String, Puffer>();
	private Puffer puffer[] = new Puffer[4];
	
	public void putInPuffer(Logdatensatz logdatensatz) {
		this.puffer.put(String.valueOf(logdatensatz.pageId), logdatensatz);
	}
	
}
