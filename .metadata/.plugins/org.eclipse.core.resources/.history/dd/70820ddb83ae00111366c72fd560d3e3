package de.dis2012.core.log;

import java.util.Hashtable;

public class Puffer {
	Hashtable<String, Logdatensatz> puffer = new Hashtable<String, Logdatensatz>();

	public Hashtable<String, Puffer> getPuffer() {
		return puffer;
	}

	public void setPuffer(Hashtable<String, Puffer> puffer) {
		this.puffer = puffer;
	}
	
	public void putInPuffer(Logdatensatz logdatensatz) {
		puffer.put("a", logdatensatz);
	}



	
}
